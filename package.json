  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "gh-pages": "^3.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
